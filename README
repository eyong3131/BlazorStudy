#Blazor Server
- dotnet new razorcomponent -n filename -o Path
    -dotnet new razorcomponent -n Todo -o Pages
    -dotnet new blazorwasm --hosted  --output EyongChatTutorial --pwa
#Blazor WebAssembly
-dotnet blazorwasm -o FolderName
    -dotnet new razorcomponent -n filename -o Path
        -dotnet new razorcomponent -n Todo -o Pages

#Build
-dotnet build

#create asp mvc
-dotnet new mvc -o Foldername

#run mssql to terminal
-sqlcmd -S localhost -U sa -P '<YourPassword>'

#entity Framwork migration command
-dotnet ef migrations add fileName
#update db with entity framwork
-dotnet ef database update 

